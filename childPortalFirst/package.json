  "scripts": {
    "clean": "rimraf dist",
    "dll": "webpack --mode production --config ./webpack/webpack.dll.js --progress --profile --colors",
    "dev": "webpack --progress --profile --colors --config ./webpack/webpack.dev.js --json > compilation-stats.json",
    "start_dev": "cross-env NODE_ENV=stage1 node --max_old_space_size=4096 node_modules/webpack-dev-server/bin/webpack-dev-server.js --config ./webpack/webpack.dev.js --progress --colors --inline --env.dev env.API_URL=http://localhost:1100",
    "build": "npm run clean && node --max_old_space_size=2048 node_modules/webpack/bin/webpack.js --progress --profile --colors --config ./webpack/webpack.prod.js",
    "build:local": "npm run clean && npm run dll && cross-env NODE_ENV=development node --max_old_space_size=2048 node_modules/webpack/bin/webpack.js --progress --profile --colors --config ./webpack/webpack.build.local.js",
    "build:dev": "npm run clean && npm run dll && node --max_old_space_size=2048 node_modules/webpack/bin/webpack.js --progress --profile --colors --config ./webpack/webpack.dev.js",
    "start:dev": "perl -pi.bak -e 's/production/stage1/g' ./dist/index.bundle.js && cross-env NODE_ENV=stage1 PORT=3000 ENV_VAR_PATH=/sid/appsrv/env_saml_cert/.env npm run start",
    "start:ci": "perl -pi.bak -e 's/production/ci/g' ./dist/index.bundle.js && cross-env NODE_ENV=ci PORT=3000 ENV_VAR_PATH=/sid/appsrv/env_saml_cert/.env npm run start",
    "start:prod": "perl -pi.bak -e 's/production/production/g' ./dist/index.bundle.js && cross-env NODE_ENV=production PORT=3000 ENV_VAR_PATH=/sid/appsrv/env_saml_cert/.env npm run start"
  },
